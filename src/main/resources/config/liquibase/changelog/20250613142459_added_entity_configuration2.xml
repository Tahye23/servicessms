    <!--
        Added the entity Contact.
    -->
    <changeSet id="20250613111356-12" author="saadna">
        <preConditions onFail="MARK_RAN" onError="HALT">
            <not>
                <columnExists tableName="configuration" columnName="coexistence_enabled"/>
