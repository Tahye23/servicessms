<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
                            http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <!--
        Added the entity SendSms.
    -->
    <changeSet id="20250430142411-1" author="jhipster">
        <preConditions onFail="MARK_RAN" onError="HALT">
            <not>
                <tableExists tableName="import_history"/>
            </not>
        </preConditions>
        <createTable tableName="import_history">
            <column name="id" type="BIGINT">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="user_login" type="VARCHAR(255)"/>
            <column name="bulk_id" type="VARCHAR(255)">
                <constraints nullable="false" unique="true"/>
            </column>
            <column name="import_date" type="TIMESTAMP WITH TIME ZONE">
                <constraints nullable="false"/>
            </column>
            <column name="total_lines" type="INTEGER">
                <constraints nullable="false"/>
            </column>
            <column name="inserted_count" type="INTEGER">
                <constraints nullable="false"/>
            </column>
            <column name="rejected_count" type="INTEGER">
                <constraints nullable="false"/>
            </column>
            <column name="duplicate_count" type="INTEGER">
                <constraints nullable="false"/>
            </column>
            <column name="status" type="VARCHAR(50)">
                <constraints nullable="false"/>
            </column>
        </createTable>

    </changeSet>

</databaseChangeLog>
